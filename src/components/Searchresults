import React from "react";
import { useNavigate } from "react-router-dom";
import { useState } from "react";
import { useParams } from "react-router-dom";
import {
  List,
  ListItem,
  ListItemSuffix,
  Chip,
  Card,
  CardHeader,
  Typography,
  Button,
  Dialog,
  DialogHeader,
  DialogBody,
  DialogFooter,
  Input,
  IconButton,
  Avatar,
  Navbar,
} from "@material-tailwind/react";
import { lists } from "../dummydata";


const SearchResults: React.FC = () => {
    const { searchKeyword } = useParams<{ searchKeyword: string }>();
  
    // Filter the lists based on the search keyword
    const filteredLists = lists.filter((list) =>
      list.title.toLowerCase().includes(searchKeyword.toLowerCase())
    );
  
    return (
      <div>
        <h1>Search Results for "{searchKeyword}"</h1>
  
        {filteredLists.length > 0 ? (
          <Card className="p-2 md:w-[65vw]">
            <List>
              {filteredLists.map((list) => (
                <ListItem key={list.id}>{list.title}</ListItem>
              ))}
            </List>
          </Card>
        ) : (
          <p>No search results found.</p>
        )}
      </div>
    );
  };
  
  export default SearchResults;
  
